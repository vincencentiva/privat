{
  "name": "code16/privat",
  "description": "Simple private access package for Laravel.",
  "authors": [{
    "name": "Philippe Lonchampt",
    "email": "philippe@code16.fr"
  }],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "laravel/framework": ">=5.5"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": ">=0.9.*",
    "phpunit/phpunit": ">=6.0",
    "doctrine/dbal": ">=2.5",
    "orchestra/testbench": ">=3.5"
  },
  "autoload": {
    "psr-4": {
      "Code16\\Privat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Code16\\Privat\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Code16\\Privat\\PrivatServiceProvider"
      ]
    }
  }
}
